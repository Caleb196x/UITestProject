{
  "compilerOptions": {
    "target": "esnext",
    "module": "commonjs",
    "experimentalDecorators": true,
    "jsx": "react",
    "sourceMap": true,
    "paths": {
      "@assets/*": ["Main/assets/*"]
    },
    "typeRoots": [
      "Typing",
      "./node_modules/@types"
    ],
    "outDir": "../JavaScript"
  },
  "include": [
    "react-umg/**/*",
    "Main/**/*",
    "Template/**/*"
  ],
}